{
  "name": "Bookmark King",
  "version": "1.0",
  "description": "Start Bookmarking like a KING",
  "permissions": [ "bookmarks", "activeTab", "cookies", "https://*.restdb.io/*", "https://www.vikyn.io/*" ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "bookmarkKing.html"
  },
  "background": {
    "scripts": [ "js/jquery.min.js", "js/background.js" ],
    "persistent": false
  },
  "manifest_version": 2
}